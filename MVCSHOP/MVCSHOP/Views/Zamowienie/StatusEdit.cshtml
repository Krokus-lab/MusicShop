@model MVCSHOP.Models.Zamowienia

@{
    ViewData["Title"] = "StatusEdit";
}

<h1>Zmień status</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="StatusEdit">
            <div class="form-group">
                <label asp-for="StanZamowienia" class="control-label"></label>
                <input asp-for="StanZamowienia" value="@ViewData["Status"]" class="form-control" />
                <span asp-validation-for="StanZamowienia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

