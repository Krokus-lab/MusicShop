@model System.Data.DataTable
@{
    ViewData["Title"] = "Index";
}

<h1>Klient</h1>

<table class="table table-bordered table-striped">
    <tr>
        <th>Tytul</th>
        <th>Autor</th>
        <th>Gatunek</th>
        <th>Zdjecie1</th>
        <th>Zdjecie2</th>
        <th>Cena</th>
        <th>Ilosc</th>
        <th>Opis</th>
        <th>Data wydania</th>
        <th></th>
    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
<tr>
    <td>@Model.Rows[i][1]</td>
    <td>@Model.Rows[i][2]</td>
    <td>@Model.Rows[i][3]</td>
    <td>@Model.Rows[i][4]</td>
    <td>@Model.Rows[i][5]</td>
    <td>@Model.Rows[i][6]</td>
    <td>@Model.Rows[i][7]</td>
    <td>@Model.Rows[i][8]</td>
    <td>@Model.Rows[i][9]</td>
    <td>
        <a href="@Url.Action("Edit", "Produkt", new { @id=Model.Rows[i][0] })">Edycja</a>/
        <a href="@Url.Action("Delete", "Produkt", new { @id=Model.Rows[i][0] })">Usuń</a>
    </td>
</tr>
    }
</table>

<a asp-action="Index">Back to List</a>